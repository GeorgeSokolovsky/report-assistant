{
  "name": "report-assistant",
  "version": "0.0.1",
  "description": "Report assistant",
  "main": "electron.js",
  "scripts": {
    "prestart": "npm run clean",
    "start": "webpack --display-error-details --progress --watch",
    "prestart:aot": "npm run clean",
    "start:aot": "webpack --display-error-details --progress --env.aot --watch",
    "electron": "npm run build:electron:main && electron ./build --serve",
    "build:electron:main": "tsc ./src/electron.ts --outDir ./build && copyfiles package.json ./build && cd ./build && npm install --prod && cd ..",
    "prebuild": "npm run clean",
    "build": "webpack --display-error-details --progress && npm run build:electron:main",
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --display-error-details -p --progress && npm run build:electron:main",
    "clean": "rimraf build",
    "app:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "app:windows": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "app:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeorgeSokolovsky/report-assistant.git"
  },
  "author": "George Sokolovsky",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "5.2.9",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "@ngtools/webpack": "1.5.2",
    "@types/csv-parse": "1.1.11",
    "@types/csv-stringify": "1.4.2",
    "@types/lodash": "4.14.106",
    "@types/node": "9.6.1",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "4.0.1",
    "circular-dependency-plugin": "3.0.0",
    "codelyzer": "3.1.2",
    "copy-webpack-plugin": "4.0.1",
    "copyfiles": "1.2.0",
    "core-js": "2.4.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "electron": "1.8.4",
    "electron-installer-dmg": "0.2.1",
    "electron-packager": "11.2.0",
    "electron-reload": "1.2.2",
    "electron-winstaller": "2.6.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "2.29.0",
    "minimist": "1.2.0",
    "node-sass": "4.8.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "rxjs": "5.5.8",
    "sass-loader": "6.0.7",
    "source-map-loader": "0.2.3",
    "style-loader": "0.20.3",
    "to-string-loader": "1.1.5",
    "typescript": "2.8.1",
    "uglify-js": "3.3.17",
    "webpack": "3.3.0",
    "webpack-merge": "4.1.0",
    "zone.js": "0.8.23"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "csv-parse": "2.1.0",
    "csv-stringify": "2.0.4",
    "lodash": "4.17.5",
    "moment": "2.18.1"
  }
}
